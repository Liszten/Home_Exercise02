<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="at.fh.swengb.lesch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="at.fh.swengb.lesch.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="at.fh.swengb.lesch.RatingMovieActivity"
            android:label="@string/rating_movie"
            android:parentActivityName="at.fh.swengb.lesch.MovieDetailsActivity" />
        <activity
            android:name="at.fh.swengb.lesch.MovieDetailsActivity"
            android:label="@string/details_name"
            android:parentActivityName="at.fh.swengb.lesch.MainActivity" />
    </application>

</manifest>